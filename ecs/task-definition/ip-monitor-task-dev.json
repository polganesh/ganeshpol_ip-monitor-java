{
  "family": "ip-monitor-awsEcs-task-dev",
  "volumes": [
		{
			"name": "efsSharedVolume",
			"host": {"sourcePath": "/home/ec2-user/apps/data/ip-monitor/"}
		}
	],
  
  "containerDefinitions": [
    {
      "name": "ip-monitor-dev",
      "image": "903856756733.dkr.ecr.eu-central-1.amazonaws.com/deutschefin/ip-monitor:dev",
      "cpu": 512,
      "memoryReservation": 512,
	  "essential": true,
	  "portMappings": [
        {
          "containerPort": 8080
          "protocol": "tcp"
        }
      ],
	  "mountPoints": [
        {
          "sourceVolume": "efsSharedVolume",
          "containerPath": "/home/ec2-user/apps/data/ip-monitor/",
		  "readOnly": false
        }
      ]
      
    }
  ]
}

